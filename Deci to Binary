#include<iostream>
#include<math.h>
using namespace std;


int main() {

    int n;
    cin>>n;
    int j=0;
    int i;
    int ans=0;
    while (n!=0){
        i=n%2;
        n=n/2;
        
        ans = ans + i*pow(10,j);
        j++;

        
        
    }
    cout<<" Answer is " << ans << endl;
    return 0;
}
